---
import Layout from "../layouts/Layout.astro";
import SocialCards from "../components/SocialCards.astro";
const getSocialData = await fetch(`http://${Astro.url.host}/api/socials.json`);
const socialData = await getSocialData.json();
console.log(socialData);
---
<Layout title="Socials">
    {
        socialData.socials.map((output: any) => (
        <SocialCards
            image={output.icon}
            name={output.name}
            link={output.link}
        />
        ))
    }
</Layout>
